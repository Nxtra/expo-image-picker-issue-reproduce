{
  "cli": {
    "version": ">= 0.46.0"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "Dev",
        "APP_NAME": "My Camera (D)",
        "PACKAGE_NAME": "be.transit.camera"
      }
    },
    "test": {
      "channel": "test",
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "Test",
        "APP_NAME": "My Camera (T)",
        "PACKAGE_NAME": "be.transit.camera.test"
      }
    }
  }
}
